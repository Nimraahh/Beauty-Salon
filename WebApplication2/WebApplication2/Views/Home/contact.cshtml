@model  WebApplication2.Models.user
<!DOCTYPE html>
<html lang="en">
    <head>
        <link rel="stylesheet" href="~/css/contact.css" asp-append-version="true" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    </head>
    

<body>

        
